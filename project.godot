; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotRuntimeInspector"
run/main_scene="res://GodotRuntimeInspector.tscn"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
config/icon="res://App.ico"
config/windows_native_icon="res://App.ico"

[autoload]

ImGuiLayer="*res://addons/imgui-godot/ImGuiLayer.tscn"

[display]

window/size/viewport_width=1618
window/size/viewport_height=1000

[dotnet]

project/assembly_name="GodotRuntimeInspector"

[editor_plugins]

enabled=PackedStringArray("res://addons/imgui-godot/plugin.cfg")
